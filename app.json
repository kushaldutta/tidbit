{
  "expo": {
    "name": "Tidbit",
    "slug": "tidbit",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/tidbit.png",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cinnamiddles.tidbit",
      "buildNumber": "4",
      "icon": "./assets/tidbit.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/tidbit.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/tidbit.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tidbit.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/tidbit.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new tidbits"
    },
    "extra": {
      "eas": {
        "projectId": "4337afcf-a02c-4ea8-b202-e3b836db1771"
      }
    }
  }
}
